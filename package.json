{
  "name": "syncday-api",
  "version": "1.1.6",
  "description": "api for syncday",
  "author": "Alan",
  "private": true,
  "license": "LICENSED",
  "options": {
    "mocha": "--config .mocharc.yml"
  },
  "scripts": {
    "bumpup": "npm version patch --no-git-tag-version",
    "bumpup:minor": "npm version minor --no-git-tag-version",
    "bumpup:major": "npm version major --no-git-tag-version",
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "prebuild:docs": "npm run test:coverage",
    "build:docs": "compodoc -p tsconfig.doc.json",
    "postbuild:docs": "cp .docs/report.html docs/additional-documentation/syncday-api-test-html-report.html",
    "prestart": "npm run build",
    "start": "ENV=local nest start --watch",
    "start:local": "ENV=local nest start --watch",
    "start:local:dev": "ENV=local_development nest start --watch",
    "start:local:staging": "ENV=staging nest start --watch",
    "prestart:local:prod": "npm run build",
    "start:local:prod": "ENV=local_production nest start --watch",
    "prestart:local:prod:pm2": "npm run build",
    "start:local:prod:pm2": "ENV=local_production pm2 start ecosystem.json",
    "start:debug": "nest start --debug --watch",
    "start:prod": "ENV=production pm2-runtime start ecosystem.json",
    "start:docker:nginx": "docker compose up nginx",
    "start:docker:mariadb": "docker compose up mariadb",
    "start:docker:redis": "docker compose up redis",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "mocha $npm_package_options_mocha 'src/**/*{.test,.spec}.ts'",
    "test:coverage": "nyc npm run test:ci",
    "test:ci": "mocha $npm_package_options_mocha -R mocha-simple-html-reporter 'src/**/*{.test,.spec}.ts'",
    "test:failonly": "mocha $npm_package_options_mocha 'src/**/*{.test,.spec}.ts' | grep -B 3 -A 3 -E '\\d)'",
    "test:single": "mocha $npm_package_options_mocha",
    "prune": "bash scripts/prune-merged-branches.sh",
    "prepare": "husky install",
    "core": "git submodule update --remote",
    "core:clean": "git submodule update --checkout -f && git submodule foreach 'git clean -df'",
    "init": "npm install && git submodule update --init --remote",
    "typeorm": "typeorm-ts-node-commonjs",
    "premigration:local": "npm run build && npm run migration:clean",
    "premigration:staging": "npm run build && npm run migration:clean",
    "premigration:production": "npm run build && npm run migration:clean",
    "migration:clean": "rm -rf migrations/*",
    "migration:local": "npm run typeorm -- migration:generate -d ./ormconfig.migration-local.ts ./migrations/local-update",
    "migration:staging": "npm run typeorm -- migration:generate -d ./ormconfig.migration-staging.ts ./migrations/staging-update",
    "migration:production": "npm run typeorm -- migration:generate -d ./ormconfig.migration-production.ts ./migrations/production-update",
    "migration:production:run": "npm run typeorm -- migration:run -d ./ormconfig.migration-production.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.300.0",
    "@aws-sdk/client-s3": "^3.304.0",
    "@aws-sdk/client-sns": "^3.363.0",
    "@aws-sdk/s3-request-presigner": "^3.369.0",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "aws-sdk": "^2.1344.0",
    "axios": "^1.3.5",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cluster-key-slot": "^1.1.2",
    "express": "^4.18.2",
    "googleapis": "^114.0.0",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.2.0",
    "nest-winston": "^1.9.1",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pm2": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "resolve-accept-language": "^1.1.49",
    "rrule": "^2.7.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.12",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-cloudwatch": "^6.1.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/express": "^4.17.17",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.41.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "mocha-simple-html-reporter": "^2.0.0",
    "nyc": "^15.1.0",
    "sinon": "^15.0.2",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typeorm-faker": "^1.3.12",
    "typescript": "^5.0.4"
  }
}
